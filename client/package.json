{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "dev": "routify -c dev-vite ",
    "dev:windows": "npm run open & routify -c dev-vite ",
    "build": "routify -b && vite build && npm run brotli:dist",
    "dev-vite": "vite",
    "serve-vite": "vite preview",
    "brotli:dist": "node br.js",
    "open": "start http://localhost:3000",
    "validate": "svelte-check",
    "format": "prettier -w src"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "@roxi/routify": "^2.13.1",
    "@svitejs/vite-plugin-svelte": "^0.9.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/html-minifier": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "autoprefixer": "^10.2.5",
    "brotli": "^1.3.2",
    "eslint": "^7.21.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "open": "^7.4.2",
    "postcss": "^8.2.7",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.1",
    "svelte-preprocess": "^4.6.9",
    "tailwindcss": "^2.0.3",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2",
    "vite": "^2.0.5",
    "vite-plugin-windicss": "^0.6.5",
    "windicss": "^2.3.0"
  },
  "dependencies": {
    "renderless-svelte": "^1.5.0"
  }
}
